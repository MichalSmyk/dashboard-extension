{
    "name": "Extension For ES Dashboard",
    "version": "0.0.0.1",
    "manifest_version": 3,
    "description": "Extension that shows data fetched from locally running server.",

    "permissions": [
        "bookmarks",
        "http://localhost/*"
    ],
    "content_scripts": [
        {
            "matches": [
              "<all_urls>"
            ],
            "js": ["lib/jquery.min.js","content-scripts/content.js"]
        }
    ]
}
